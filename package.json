  "jest": {
    "verbose": true,
    "globals": {
      "PIWIK_SITE": 2,
      "PIWIK_INSTANCE": "http://localhost/mock-piwik/",
      "API_ENDPOINT": "/assets/snapshot-data.json",
      "GOOGLE_API_KEY": "blahblahblah"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|css)$": "<rootDir>/jest-mock-styles.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/**",
      "!src/index.js",
      "!src/store/**/*"
    ]
  },
  "dependencies": {}
}

{
  "name": "boac-frontend",
  "version": "1.0.0",
  "description": "lef boac frontend",
  "repository": "https://gitlab.wgtn.cat-it.co.nz/lef/boac-frontend.git",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "node build.js",
    "build:testdata": "node build.js --piwik 2 --endpoint /assets/test-data.json",
    "build:externaltest": "node build.js --piwik 2 --endpoint https://govtnz-test1.cwp.govt.nz/BoacAPI/v1/all",
    "build:externalprod": "node build.js --piwik 2 --endpoint https://govtnz.cwp.govt.nz/BoacAPI/v1/all",
    "start": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint https://govtnz.cwp.govt.nz/BoacAPI/v1/all",
    "start:externaltest": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint https://govtnz-test1.cwp.govt.nz/BoacAPI/v1/all",
    "start:testdata": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint /assets/test-data.json",
    "start:snapshot": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint /assets/snapshot-data.json",
    "lint": "./node_modules/.bin/standard",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "scss",
    "es6",
    "es2015"
  ],
  "author": "Catalyst",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.3.7",
    "babel-core": "~6.10.4",
    "babel-eslint": "~6.1.2",
    "babel-jest": "~16.0.0",
    "babel-loader": "~6.2.4",
    "babel-polyfill": "~6.13.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-2": "~6.17.0",
    "classnames": "~2.2.5",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "~0.23.1",
    "enzyme": "~2.4.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.22.0",
    "is-git-clean": "~1.0.0",
    "jest": "~16.0.1",
    "node-sass": "~3.8.0",
    "postcss-loader": "~0.9.1",
    "react": "~15.3.0",
    "react-addons-test-utils": "~15.3.2",
    "react-cookie": "~0.4.7",
    "react-dom": "~15.3.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.8.1",
    "react-test-renderer": "~15.3.2",
    "redux": "~3.5.2",
    "redux-thunk": "~2.1.0",
    "sass-loader": "~4.0.0",
    "standard": "~7.1.2",
    "style-loader": "~0.13.1",
    "url-search-params": "~0.5.0",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-merge": "~0.14.0",
    "whatwg-fetch": "~1.0.0",
    "yargs": "~4.8.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "PIWIK_SITE": 2,
      "PIWIK_INSTANCE": "http://localhost/mock-piwik/",
      "API_ENDPOINT": "/assets/test-data.json"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|css)$": "<rootDir>/jest-mock-styles.js"
    },
    "collectCoverageFrom": ["src/**/*.{js,jsx}", "!node_modules/**", "!src/index.js", "!src/store/**/*"]
  }
}

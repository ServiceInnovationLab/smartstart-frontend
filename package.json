{
  "name": "boac-frontend",
  "version": "1.0.0",
  "description": "lef boac frontend",
  "repository": "https://gitlab.wgtn.cat-it.co.nz/lef/boac-frontend.git",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": " ./node_modules/.bin/jest --watch",
    "lint": "./node_modules/.bin/eslint src",
    "coverage": "./node_modules/.bin/jest --coverage",
    "build": "node build.js",
    "build:remotecontent": "node build.js --piwik 2 --endpoint https://www.govt.nz/BoacAPI/v1/all",
    "start": "./node_modules/.bin/webpack-dev-server --inline --piwik 2 --endpoint /api/request/content/timeline",
    "start:remotecontent": "./node_modules/.bin/webpack-dev-server --inline --piwik 2 --endpoint https://www.govt.nz/BoacAPI/v1/all",
    "start:snapshot": "./node_modules/.bin/webpack-dev-server --inline --piwik 2 --endpoint /assets/snapshot-data.json"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "scss",
    "es6",
    "es2015"
  ],
  "author": "Catalyst",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.5.2",
    "babel-core": "~6.18.2",
    "babel-eslint": "~7.1.1",
    "babel-jest": "~16.0.0",
    "babel-loader": "~6.2.7",
    "babel-polyfill": "~6.16.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-2": "~6.18.0",
    "classnames": "~2.2.5",
    "copy-webpack-plugin": "~4.0.0",
    "css-loader": "~0.25.0",
    "d3": "~4.4.0",
    "deepmerge": "^2.0.1",
    "enzyme": "~2.5.1",
    "eslint": "~3.11.1",
    "eslint-config-standard": "~6.2.1",
    "eslint-plugin-promise": "~3.4.0",
    "eslint-plugin-react": "~6.7.1",
    "eslint-plugin-standard": "~2.0.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "fetch-retry": "~1.1.1",
    "geolib": "~2.0.23",
    "google-map-react": "~0.26.0",
    "html-webpack-plugin": "~2.24.1",
    "is-git-clean": "~1.1.0",
    "jest": "~16.0.2",
    "js-md5": "~0.4.1",
    "lodash": "~4.17.4",
    "moment": "~2.18.1",
    "node-sass": "~3.11.2",
    "postcss-loader": "~1.1.0",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-async-script-loader": "~0.2.2",
    "react-autosuggest": "~9.0.1",
    "react-cookie": "~0.4.8",
    "react-dom": "~15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-nl2br": "~0.2.0",
    "react-redux": "~4.4.5",
    "react-router": "~3.0.0",
    "react-scroll": "~1.4.8",
    "react-scrollable-anchor": "^0.4.2",
    "react-select": "1.0.0-rc.5",
    "react-sticky": "~6.0.1",
    "react-test-renderer": "~15.3.2",
    "react-transition-group": "~1.1.3",
    "redux": "~3.6.0",
    "redux-form": "~6.6.3",
    "redux-logger": "~3.0.1",
    "redux-thunk": "~2.1.0",
    "sass-loader": "~4.0.2",
    "standard": "~8.5.0",
    "style-loader": "~0.13.1",
    "throttle-debounce": "~1.0.1",
    "url-search-params": "~0.6.1",
    "uuid": "~3.0.1",
    "webpack": "~1.13.3",
    "webpack-dev-server": "~1.16.2",
    "webpack-merge": "~4.1.0",
    "whatwg-fetch": "~1.0.0",
    "yargs": "~6.3.0"

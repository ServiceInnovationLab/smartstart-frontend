{
  "name": "boac-frontend",
  "version": "1.0.0",
  "description": "lef boac frontend",
  "repository": "https://gitlab.wgtn.cat-it.co.nz/lef/boac-frontend.git",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "node build.js",
    "build:testdata": "node build.js --piwik 2 --endpoint /assets/test-data.json",
    "build:externaltest": "node build.js --piwik 2 --endpoint https://govtnz-test1.cwp.govt.nz/BoacAPI/v1/all",
    "build:externalprod": "node build.js --piwik 2 --endpoint https://www.govt.nz/BoacAPI/v1/all",
    "start": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint https://www.govt.nz/BoacAPI/v1/all",
    "start:externaltest": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint https://govtnz-test1.cwp.govt.nz/BoacAPI/v1/all",
    "start:testdata": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint /assets/test-data.json",
    "start:snapshot": "./node_modules/.bin/webpack-dev-server --quiet --hot --inline --piwik 2 --endpoint /assets/snapshot-data.json",
    "lint": "./node_modules/.bin/eslint src",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "scss",
    "es6",
    "es2015"
  ],
  "author": "Catalyst",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.5.2",
    "babel-core": "~6.18.2",
    "babel-eslint": "~7.1.1",
    "babel-jest": "~16.0.0",
    "babel-loader": "~6.2.7",
    "babel-polyfill": "~6.16.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-2": "~6.18.0",
    "classnames": "~2.2.5",
    "copy-webpack-plugin": "~4.0.0",
    "css-loader": "~0.25.0",
    "d3": "~4.4.0",
    "enzyme": "~2.5.1",
    "eslint": "~3.11.1",
    "eslint-config-standard": "~6.2.1",
    "eslint-plugin-promise": "~3.4.0",
    "eslint-plugin-react": "~6.7.1",
    "eslint-plugin-standard": "~2.0.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.24.1",
    "is-git-clean": "~1.1.0",
    "jest": "~16.0.2",
    "js-md5": "~0.4.1",
    "node-sass": "~3.11.2",
    "postcss-loader": "~1.1.0",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-cookie": "~0.4.8",
    "react-dom": "~15.3.2",
    "react-redux": "~4.4.5",
    "react-router": "~3.0.0",
    "react-scrollable-anchor": "git+https://github.com/alexey0511/react-scrollable-anchor.git#bc957aae041f2d4f8e4f9b9ce66635ded6a6b204",
    "react-test-renderer": "~15.3.2",
    "redux": "~3.6.0",
    "redux-thunk": "~2.1.0",
    "sass-loader": "~4.0.2",
    "standard": "~8.5.0",
    "style-loader": "~0.13.1",
    "throttle-debounce": "~1.0.1",
    "url-search-params": "~0.6.1",
    "webpack": "~1.13.3",
    "webpack-dev-server": "~1.16.2",
    "webpack-merge": "~0.15.0",
    "whatwg-fetch": "~1.0.0",
    "yargs": "~6.3.0"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "PIWIK_SITE": 2,
      "PIWIK_INSTANCE": "http://localhost/mock-piwik/",
      "API_ENDPOINT": "/assets/test-data.json"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|css)$": "<rootDir>/jest-mock-styles.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/**",
      "!src/index.js",
      "!src/store/**/*"
    ]
  }
}
